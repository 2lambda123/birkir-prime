    "antd": "3.16.1",
    "apollo-server-core": "^2.3.1",
    "braft-editor": "2.2.9",
    "draft-js": "^0.10.5",
    "draft-js-export-html": "^1.3.3",
    "draft-js-export-markdown": "^1.3.3",
    "draft-js-import-markdown": "^1.3.3",
    "graphql": "^14.1.1",
    "graphql-type-json": "^0.3.0",
