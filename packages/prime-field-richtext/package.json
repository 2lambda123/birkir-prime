  "homepage": "https://github.com/birkir/prime/tree/master/packages/prime-field-richtext",
  "license": "MIT",
  "main": "lib/index",
  "typings": "lib/index",
  "prime": "lib/ui",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "start": "tsc --watch; webpack --config ../prime-tools/webpack.config.js --watch",
